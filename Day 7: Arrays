#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,i,j,a[100],b[100];
    cin>>n;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
    }
    i=n-1;
    for(j=0;j<n;i--,j++)
    {
        b[j]=a[i];
    }
    for(j=0;j<n;j++)
    {
        cout<<b[j]<<" ";
    }
    return 0;
   
}

