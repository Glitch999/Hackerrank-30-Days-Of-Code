#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
#include <cstring>
using namespace std;


int main() {
    int x;
    cin>>x;
    
    for(int i=0; i<x; i++)
    {
        string s;
        cin>>s;
        for(int i=0;i<s.size();i++)
        {
            if(i%2==0)
            cout<<s[i];
        }
        cout<<" ";
        for(int i=0;i<s.size();i++)
        {
            if(i%2!=0)
            cout<<s[i];
        }
        cout<<"\n";
    }
    return 0;
}
